FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue tracer 0
);

regions
(
    boxToCell
    {
        box (0 0 -1) (8 0.5 1);

        fieldValues
        (
            volScalarFieldValue tracer 1
        );
    }
);

