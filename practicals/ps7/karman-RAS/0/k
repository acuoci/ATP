FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1944;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1944;
    }
    outlet
    {
        type            zeroGradient;
    }
    topAndBottom
    {
        type            zeroGradient;
    }
    cylinder
    {
        type            kqRWallFunction;
        value           uniform 1944;
    }
    frontAndBack
    {
        type            empty;
    }
}


